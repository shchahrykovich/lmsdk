{
  "name": "lmsdk",
  "private": true,
  "version": "0.0.8",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint-with-warnings": "eslint",
    "lint": "eslint . --quiet --max-warnings=0",
    "preview": "npm run build && vite preview",
    "db:version": "wrangler d1 time-travel info DB",
    "db:migrations:apply": "wrangler d1 migrations apply DB --remote",
    "deploy": "npm run db:version && npm run db:migrations:apply && npm run build && wrangler deploy",
    "deploy-from-local": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ai": "vitest run --reporter=./tests/vitest-ai-reporter.ts"
  },
